; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Zoltak"
run/main_scene="res://Scene/Start Menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GlobalGameSystem="*res://Scene/global/global-game_system.tscn"
SceneTransition="*res://Scene/scene_transition.tscn"
LevelManager="*res://00_glabal/level_manager.gd"
SignalManager="*res://00_glabal/signal_manager.gd"

[display]

window/size/viewport_width=1080
window/size/viewport_height=2160
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[file_customization]

folder_colors={
"res://Scene/00_default_class_item_load/": "gray",
"res://Scene/Stories/": "green",
"res://Scene/achievements/": "teal",
"res://Scene/action/": "red",
"res://Scene/battle/": "pink",
"res://Scene/inventory/": "yellow",
"res://Scene/journal/": "blue",
"res://Scene/shop/": "purple"
}

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
